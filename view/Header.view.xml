<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="demo.FinalProject.controller.Header">
	<App>
		<pages>
			<Page title="Title">
				<content>
				  
				</content>
			<customHeader>
			     <Toolbar width="100%" id="toolbar1">
						<content>
							<HBox width="100%" id="hbox0">
								<items>
									<HBox width="100%" id="hbox1">
										<items>
											<FlexBox height="12px"/>
											<Image class="marginTop myImage" src="{Photo}" width="47px" id="image0"/>
											<FlexBox width="10px"/>
											<VBox class="marginTop" width="100%" direction="Column" id="vbox0">
												<items>
													<Title class="name" text="{Name}"/>
													<Title class="role" text="{Role}" id="text1"/>
												</items>
											</VBox>
										</items>
									</HBox>
									<HBox alignItems="Center">
										<Image src="data:image/png;base64," width="70px" id="image1"/>
										<FlexBox width="7px"/>
									</HBox>
								</items>
							</HBox>
						</content>
					</Toolbar>
			</customHeader>
			</Page>
		</pages>
	</App>
</mvc:View>